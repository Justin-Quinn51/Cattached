// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id Int @id @default(autoincrement())
  firstName String 
  lastName String
  email String @unique
}

model Cat {
  id Int @id @default(autoincrement())
  name String
  foundAt String
  aged Int 
  breed Breed?
}

model Breed {
  id Int @id @default(autoincrement())
  cat Cat @relation(fields: [catId], references: [id])
  catId Int @unique
  descript String
  traits String[]
  lifespan String
  origin String
  weight String
}

